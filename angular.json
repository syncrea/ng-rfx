{
  "version": 1,
  "projects": {
    "rfx-docs": {
      "projectType": "application",
      "root": "apps/rfx-docs",
      "sourceRoot": "apps/rfx-docs/src",
      "prefix": "rfx-docs",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/rfx-docs",
            "index": "apps/rfx-docs/src/index.html",
            "main": "apps/rfx-docs/src/main.ts",
            "polyfills": "apps/rfx-docs/src/polyfills.ts",
            "tsConfig": "apps/rfx-docs/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/rfx-docs/src/favicon.ico",
              "apps/rfx-docs/src/assets"
            ],
            "styles": ["apps/rfx-docs/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/rfx-docs/src/environments/environment.ts",
                  "with": "apps/rfx-docs/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "rfx-docs:build:production"
            },
            "development": {
              "browserTarget": "rfx-docs:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "rfx-docs:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/rfx-docs/src/**/*.ts",
              "apps/rfx-docs/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/rfx-docs"],
          "options": {
            "jestConfig": "apps/rfx-docs/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "rfx-lib": {
      "projectType": "library",
      "root": "libs/rfx-lib",
      "sourceRoot": "libs/rfx-lib/src",
      "prefix": "rfx",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/rfx-lib"],
          "options": {
            "project": "libs/rfx-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/rfx-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/rfx-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/rfx-lib"],
          "options": {
            "jestConfig": "libs/rfx-lib/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/rfx-lib/src/**/*.ts",
              "libs/rfx-lib/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "rfx-docs"
}
